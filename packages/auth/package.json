{
  "name": "@acme/auth",
  "version": "2.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./env": "./src/env.ts",
    "./providers/*": "./src/providers/*.ts",
    "./user-handler": "./src/user-handler.ts"
  },
  "scripts": {
