{
  "name": "@acme/locales",
  "version": "2.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./lang/*": "./src/lang/*.json",
    "./react": "./src/react.tsx",
    "./routing": "./src/routing.ts"
  },
  "scripts": {
    "_build": "pnpm run generate",
    "_dev": "nodemon --watch ./src/lang --ext json --exec \"pnpm run generate\"",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "clean:cache": "git clean -xdf .cache",
    "format": "prettier --check . '!src/generated/**' --ignore-path ../../.gitignore",
    "generate": "node --import tsx/esm ./src/generate.ts",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "prettier": "@acme/prettier-config",
  "dependencies": {
    "next-intl": "3.26.3",
    "react": "19.0.0"
  },
  "devDependencies": {
    "@acme/eslint-config": "workspace:*",
    "@acme/prettier-config": "workspace:*",
    "@acme/tailwind-config": "workspace:*",
    "@acme/tsconfig": "workspace:*",
    "@types/react": "19.0.8",
    "eslint": "9.19.0",
    "nodemon": "3.1.9",
    "prettier": "3.4.2",
    "tsx": "4.19.2",
    "typescript": "5.7.3"
  }
}
