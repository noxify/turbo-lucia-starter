{
  "name": "@acme/ui",
  "version": "2.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*.tsx",
    "./combobox": "./src/combobox/index.tsx",
    "./combobox/*": "./src/combobox/*.tsx",
    "./data-table": "./src/data-table/index.tsx",
    "./data-table/*": "./src/data-table/*.tsx",
    "./hooks/*": "./src/hooks/*.ts"
  },
  "scripts": {
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "clean:cache": "git clean -xdf .cache",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "ui-add": "pnpm dlx shadcn@latest add && prettier src --write --list-different"
  },
  "prettier": "@acme/prettier-config",
  "dependencies": {
    "@acme/helpers": "workspace:*",
    "@acme/locales": "workspace:*",
    "@acme/search-params": "workspace:*",
    "@hookform/resolvers": "3.9.1",
    "@radix-ui/react-alert-dialog": "1.1.4",
    "@radix-ui/react-avatar": "1.1.2",
    "@radix-ui/react-checkbox": "1.1.3",
    "@radix-ui/react-collapsible": "1.1.2",
    "@radix-ui/react-dialog": "1.1.4",
    "@radix-ui/react-dropdown-menu": "2.1.4",
    "@radix-ui/react-icons": "1.3.2",
    "@radix-ui/react-label": "2.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.3",
    "@radix-ui/react-popover": "1.1.4",
    "@radix-ui/react-radio-group": "1.2.2",
    "@radix-ui/react-scroll-area": "1.2.2",
    "@radix-ui/react-select": "2.1.4",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "1.1.1",
    "@radix-ui/react-switch": "1.1.2",
    "@radix-ui/react-toast": "1.2.4",
    "@radix-ui/react-tooltip": "1.1.6",
    "@sindresorhus/slugify": "2.2.1",
    "@tanstack/react-table": "8.20.6",
    "class-variance-authority": "0.7.1",
    "cmdk": "1.0.0",
    "date-fns": "4.1.0",
    "deepsearchjs": "1.7.15",
    "lucide-react": "0.468.0",
    "next-themes": "0.4.4",
    "nuqs": "2.2.3",
    "react-day-picker": "8.10.1",
    "react-hook-form": "7.54.1",
    "react-querybuilder": "8.0.0",
    "sonner": "1.7.1",
    "tailwind-merge": "2.5.5",
    "tailwindcss-animate": "1.0.7",
    "vaul": "1.1.2",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@acme/eslint-config": "workspace:*",
    "@acme/prettier-config": "workspace:*",
    "@acme/tailwind-config": "workspace:*",
    "@acme/tsconfig": "workspace:*",
    "@types/react": "19.0.2",
    "eslint": "9.17.0",
    "prettier": "3.4.2",
    "react": "19.0.0",
    "tailwindcss": "3.4.17",
    "typescript": "5.7.2"
  }
}
